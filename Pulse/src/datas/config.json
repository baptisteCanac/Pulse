{
    "headManager": {
        "title": "Pulse",
        "global": {
            "tags": [
                "meta",
                "meta"
            ],
            "attributes": [
                {
                    "charset": "UTF-8"
                },
                {
                    "name": "viewport",
                    "content": "width=device-width, initial-scale=1.0"
                }
            ]
        },
        "settings": {
            "tags": [
                "link",
                "script"
            ],
            "attributes": [
                {
                    "rel": "stylesheet",
                    "type": "text/css",
                    "href": "../stylesheets/settings.css"
                },
                {
                    "type": "module",
                    "src": "../js/pages/settings.js",
                    "defer": "true"
                }
            ]
        },
        "reader": {
            "tags": [
                "script",
                "link",
                "link",
                "link"
            ],
            "attributes": [
                {
                    "type": "module",
                    "src": "../js/pages/reader.js",
                    "defer": "true"
                },
                {
                    "rel": "stylesheet",
                    "type": "text/css",
                    "href": "../stylesheets/lib/global.css"
                },
                {
                    "rel": "stylesheet",
                    "type": "text/css",
                    "href": "../stylesheets/pages/reader.css"
                },
                {
                    "rel": "stylesheet",
                    "type": "text/css",
                    "href": "../stylesheets/presentation.css"
                }
            ]
        }
    }
}